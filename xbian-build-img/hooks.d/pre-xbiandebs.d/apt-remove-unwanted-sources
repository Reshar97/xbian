
case $config_platform_branch in
    imx6-trusty|rk)
        ;;
    rpi2*|rpi3*|imx6*)
        if grep -qE "http://http.debian.net/debian|http://httpredir.debian.org/debian" ./rootfs/etc/apt/sources.list; then
            sed -i '/http:\/\/ftp.*/d' ./rootfs/etc/apt/sources.list
        fi
        ;;
    rpi*|bpi*)
        sed -i '/debian/d' ./rootfs/etc/apt/sources.list
        ;;
    *)
        ;;
esac
