
case $config_platform_branch in
    imx6-trusty)
        ;;
    rpi|bpi*)
        sed -i '/debian/d' ./rootfs/etc/apt/sources.list
        ;;
    rpi2)
        if grep -q "http://http.debian.net/debian" ./rootfs/etc/apt/sources.list; then
            sed -i '/http:\/\/ftp.*/d' ./rootfs/etc/apt/sources.list
        fi
        ;;
    imx6*|rk)
        ;;
    *)
        ;;
esac
